// Autogenerated from device-runner-linux.tmpl

package main

import (
	"os"

	"github.com/merliot/thing2"
	"github.com/merliot/thing2/models"
)

func main() {
	thing2.Setenv("PORT", "{{.Vars.port}}")
	thing2.Setenv("USER", "{{.Vars.user}}")
	thing2.Setenv("PASSWD", "{{.Vars.passwd}}")
	thing2.Setenv("DIAL_URLS", "{{.Vars.dialurls}}")

	thing2.Setenv("DEVICES", `{
	"{{.Device.Id}}": {
		"Id": "{{.Device.Id}}",
		"Model": "{{.Device.Model}}",
		"Name": "{{.Device.Name}}",
		"Children": [],
		"DeployParams": "{{.Device.DeployParams}}"
	}
}`)
{{ if .Vars.progenitive }}
	thing2.Models = models.AllModels
{{- else }}
	thing2.Models = thing2.ModelMap{
		"{{.Device.Model}}": models.{{title .Device.Model}},
	}
{{- end }}

	thing2.Run()
}
